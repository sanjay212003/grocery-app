{
  "version": 2,
  "builds": [
    {
      "src": "backend/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/app.js"
    }
  ],
  "functions": {
    "backend/app.js": {
      "maxDuration": 10
    }
  }
}
